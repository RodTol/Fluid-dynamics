FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    inlet
    {
        type            cyclic;
        inGroups        1(cyclic);
        nFaces          100;
        startFace       99400;
        matchTolerance  0.0001;
        transform       unknown;
        neighbourPatch  outlet;
    }
    outlet
    {
        type            cyclic;
        inGroups        1(cyclic);
        nFaces          100;
        startFace       99500;
        matchTolerance  0.0001;
        transform       unknown;
        neighbourPatch  inlet;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          1000;
        startFace       99600;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          100000;
        startFace       100600;
    }
)

